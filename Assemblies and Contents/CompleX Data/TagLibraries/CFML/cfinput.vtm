		<attriboption value="image/x-portable-greymap" caption="image/x-portable-greymap"/>
		<attriboption value="image/x-portablebitmap" caption="image/x-portablebitmap"/>
			</attrib>

		<event name="onfocus"/>
			<event name="onblur"/>
			<event name="onselect"/>
			<event name="onchange"/>
			<event name="onclick"/>
			<event name="ondblclick"/>
			<event name="onmousedown"/>
			<event name="onmouseup"/>
			<event name="onmouseover"/>
			<event name="onmousemove"/>
			<event name="onmouseout"/>
			<event name="onkeypress"/>
			<event name="onkeydown"/>
			<event name="onkeyup"/>
   </attributes>

   <attribcategories>
      <attribgroup name="general" elements="name,type,value"/>
      <attribgroup name="appearance" elements="height,width,size,maxlength"/>
      <attribgroup name="special" elements="validate,range,required,message,checked,notab,tabindex,onvalidate,pattern,passthrough,bind,label,mask,validateat,src,daynames,monthnames,firstdayofweek"/>
 	  <attribgroup name="events" elements="onerror,onvalidate,onkeyup,onkeydown,onmouseup,onmousedown,onchange,onclick"/>
      <attribgroup name="misc" elements=""/>
   </attribcategories>


</tag>

