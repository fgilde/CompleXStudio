<tag name="cfinput" bind="value" endtag="no">
<tagformat nlbeforetag="1" nlaftertag="1"/>
<tagdialog file="cfinput.htm" />

   <attributes>
      <attrib name="type" type="enumerated">
         <attriboption value="text"/>
         <attriboption value="radio"/>
         <attriboption value="checkbox"/>
         <attriboption value="password"/>
		 <attriboption value="button"/>
		 <attriboption value="file"/>
		 <attriboption value="hidden"/>
		 <attriboption value="image"/>
		 <attriboption value="reset"/>
		 <attriboption value="submit"/>
		 <attriboption value="datefield"/>
      </attrib>

	  <attrib name="bind" type="text"/>
	  <attrib name="name"  type="text"/>
	  <attrib name="label" type="text"/>
      <attrib name="message" type="text"/>
	  <attrib name="tooltip" type="text"/>

	  <attrib name="passthrough" type="text"/>

      <attrib name="notab" type="enumerated">
         <attriboption value="yes"/>
         <attriboption value="no"/>
      </attrib>
	  <attrib name="visible" type="enumerated">
         <attriboption value="yes"/>
         <attriboption value="no"/>
      </attrib>

	  <attrib name="range" type="text"/>
      <attrib name="pattern" type="text"/>
 	  <attrib name="onvalidate" type="text"/>
	  <attrib name="mask" type="text"/>
	  <attrib name="validateat" type="enumerated">
	  	 <attriboption value="onsubmit"/>
         <attriboption value="onserver"/>
		 <attriboption value="onblur"/>
      </attrib>
	  <attrib name="validate" type="enumerated">
          <attriboption value="" />
		  <attriboption value="boolean" />
		  <attriboption value="creditcard" />
          <attriboption value="date" />
		  <attriboption value="email" />
          <attriboption value="eurodate" />
		  <attriboption value="float" />
		  <attriboption value="guid" />
		  <attriboption value="integer" />
		  <attriboption value="maxlength" />
		  <attriboption value="noblanks" />
		  <attriboption value="range" />
		  <attriboption value="regular_expression" />
		  <attriboption value="social_security_number"/>
		  <attriboption value="submitonce"/>
          <attriboption value="time" />
          <attriboption value="telephone" />
		  <attriboption value="url" />
		  <attriboption value="usdate" />
		  <attriboption value="uuid" />
          <attriboption value="zipcode" />
      </attrib>
	  <attrib name="required" type="enumerated">
         <attriboption value="yes"/>
         <attriboption value="no"/>
      </attrib>

      <attrib name="checked" type="enumerated">
         <attriboption value="yes"/>
         <attriboption value="no"/>
      </attrib>

	  <attrib name="firstdayofweek" type="enumerated">
          <attriboption value="" />
		  <attriboption value="0" />
		  <attriboption value="1" />
          <attriboption value="2" />
		  <attriboption value="3" />
          <attriboption value="4" />
		  <attriboption value="5" />
		  <attriboption value="6" />
      </attrib>
	  <attrib name="daynames" type="text"/>
	  <attrib name="monthnames" type="text"/>

	  <attrib name="onerror" type="text"/>

		<attrib name="disabled" type="flag"/>
		<attrib name="class" type="cssstyle"/>
		<attrib name="id" type="cssid"/>
		<attrib name="style" type="style"/>
		<attrib name="accesskey"/>
		<attrib name="tabindex"/>
		<attrib name="title"/>
		<attrib name="dir" type="enumerated">
		<attriboption value="ltr" caption="left-to-right"/>
		<attriboption value="rtl" caption="right-to-left"/>
			</attrib>
		<attrib name="lang" type="enumerated">
		<attriboption value="af" caption="af afrikaans"/>
		<attriboption value="sq" caption="sq albanian"/>
		<attriboption value="ar" caption="ar arabic (needs subtype)"/>
		<attriboption value="eu" caption="eu basque"/>
		<attriboption value="br" caption="br breton"/>
		<attriboption value="bg" caption="bg bulgarian"/>
		<attriboption value="be" caption="be belarusian"/>
		<attriboption value="ca" caption="ca catalan"/>
		<attriboption value="zh" caption="zh chinese (needs subtype)"/>
		<attriboption value="hr" caption="hr croatian"/>
		<attriboption value="cs" caption="cs czech"/>
		<attriboption value="da" caption="da danish"/>
		<attriboption value="nl" caption="nl dutch (standard)"/>
